<h1><%= "#{current_user.username.capitalize}'s Entries" %></h1>
<h2 id="month">
	<%= link_to "<", date: @date.prev_month %> 
	<%= @date.strftime("%B %Y") %>
	<%= link_to ">", date: @date.next_month %></h2>
<%= calendar @date do |date| %>
<%= date.day %>
	<% if @entries_by_date[date] %>
		<ul class="entries">
			<% @entries_by_date[date].each do |entry| %>
			<li><%= link_to entry.subject, entry %></li>
			<% end %>
		</ul>
	<% end %>
<% end %>